{
  "name": "myapi-mem",
  "version": "1.0.0",
  "description": "Experimental API using mem",
  "main": "server.ts",
  "directories": {
    "bin": "",
    "doc": "",
    "example": "",
    "lib": "",
    "man": "",
    "test": "test"
  },
  "man": [],
  "scripts": {
    "prebuild": "npm run temp",
    "build": "tsc",
    "postbuild": "npm run dist",
    "precache-clean": "npm run clean-node_modules",
    "cache-clean": "npm cache clean --force",
    "postcache-clean": "npm i",
    "clean-node_modules": "rm -Rf ./node_module",
    "precoverage": "npx browserslist@latest --update-db",
    "coverage": "nyc --reporter=lcov npm run test",
    "docker-build": "docker-compose build",
    "predocker-dev": "npm run docker-build",
    "docker-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker-dev-watch": "nodemon --watch . --ext ts --exec \"npm run docker-dev\"",
    "predocker": "npm docker-build",
    "docker": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "predist": "rm -Rf ./dist",
    "dist": "mv ./temp ./dist",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "serve-coverage": "npx http-server coverage/lcov-report/",
    "serve": "nodemon server.ts -e ts",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "pretemp": "rm -Rf ./temp",
    "temp": "mkdir ./temp",
    "test": "npm run test-mocha",
    "test-mocha": "mocha -r ts-node/register ./test/* ./test/**/* --exit",
    "test-dev": "nodemon --watch . --ext ts --exec \"npm run test-mocha\"",
    "prerebuild-node_modules": "npm run clean-node_modules",
    "rebuild-node_modules": "npm i"
  },
  "keywords": [
    "API",
    "ExpressJS",
    "Mockup",
    "NodeJS",
    "REST"
  ],
